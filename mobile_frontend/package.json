{
  "name": "reactnative",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "// note": "Expo-managed workflow: no native folders unless prebuild is run.",
    "build:web": "expo export --platform web",
    "build": "npm run build:web",
    "build:native": "echo \"Native build skipped: run 'npx expo prebuild' first to generate android/ios folders, then build with platform tools.\" && exit 0"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "expo": "~53.0.20",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.40.0"
  },
  "private": true
}
